{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Azure DevOps",
      "program": "${workspaceFolder}/cli.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node",
      "env": {
        "AZURE_PERSONAL_ACCESS_TOKEN": "azurePersonalAccessToken",
        "BUILD_REPOSITORY_ID": "suggestion-bot",
        "SYSTEM_PULLREQUEST_PULLREQUESTID": "13",
        "SYSTEM_TEAMFOUNDATIONCOLLECTIONURI": "https://dev.azure.com/fabrikamfiber",
        "SYSTEM_TEAMPROJECTID": "tido64"
      }
    },
    {
      "name": "GitHub",
      "program": "${workspaceFolder}/cli.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node",
      "env": {
        "GITHUB_SHA": "f3ddadcc488fe0d4ac650a0b3a5aeb72a8cb30cb",
        "GITHUB_TOKEN": "githubToken"
      }
    },
    {
      "name": "Debug Jest Tests",
      "type": "node",
      "request": "launch",
      "runtimeArgs": [
        "--inspect-brk",
        "${workspaceRoot}/node_modules/.bin/jest",
        "--runInBand"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "port": 9229
    }
  ]
}
